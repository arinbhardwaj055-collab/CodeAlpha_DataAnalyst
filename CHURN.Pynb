   "source": [
    "\n",
    "# Create a crosstab of counts\n",
    "ct = pd.crosstab(df[\"SeniorCitizen\"], df[\"Churn\"])\n",
    "\n",
    "# Convert to percentages of total\n",
    "ct_perc = ct.div(ct.sum(axis=1), axis=0) * 100\n",
    "\n",
    "# Plot stacked bar chart\n",
    "ax = ct_perc.plot(\n",
    "    kind=\"bar\", stacked=True, figsize=(5,5), colormap=\"Set2\", edgecolor=\"black\"\n",
    ")\n",
    "\n",
    "# Add labels\n",
    "for c in ax.containers:\n",
    "    # Only show non-zero labels\n",
    "    labels = [f\"{v.get_height():.1f}%\" if v.get_height() > 0 else \"\" for v in c]\n",
    "    ax.bar_label(c, labels=labels, label_type=\"center\", fontsize=9)\n",
    "\n",
    "plt.title(\"Churn by Senior Citizen (as % of total)\", fontsize=12)\n",
    "plt.xlabel(\"Senior Citizen (0 = No, 1 = Yes)\", fontsize=10)\n",
    "plt.ylabel(\"Percentage\", fontsize=10)\n",
    "plt.legend(title=\"Churn\", loc=\"upper right\")\n",
    "plt.tight_layout()\n",
    "plt.show()"
   ]
